@model Wedding

<nav>
    <h2>@Model.WedderOne & @Model.WedderTwo Wedding</h2> <div> <a href="/dashboard">Dashboard</a> <span> | </span> <a href="/logout">Logout</a> </div> 
</nav>

<div class="mt-5">
    <h4>Date: @Model.Date.Date.ToString("MMM dd yyyy")</h4>

    <div class="mt-4">
        <h4>Guests: </h4>
        <ul class="ml-3" >
            @{
                foreach( Reservation u in Model.Guests)
                {
                    <li>@u.WeddingGuest.FirstName @u.WeddingGuest.LastName</li>
                }
            }
        </ul>
    </div>
</div>